        <if test='vname != null and vname != "" and  vname != "null"'>
            and a.name like "%"#{vname}"%"
        </if>
        order by a.id desc
    </select>

    <select id="getAppointmentListByVPhone" parameterType="RespVisitor" resultType="RespVisitor">
        SELECT
         <include refid="select_respVisitor"></include>
