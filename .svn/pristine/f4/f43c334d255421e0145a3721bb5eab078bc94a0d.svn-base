        </if>
        <if test='plateNum != null and plateNum != "" and  plateNum != "null"'>
            plateNum=#{plateNum, jdbcType=VARCHAR},
        </if>
        <if test='vcompany != null and vcompany != "" and  vcompany != "null"'>
