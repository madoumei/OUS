        </if>
        <if test='chartStatus == "2"'>
            DATE_FORMAT(a.`appointmentDate`,'%H') AS hour,
        </if>
        b.`deptid` as deptId,a.userid , COUNT(a.`empid`)AS COUNT
        FROM
        qcv_visitor a,
        qcv_emp_dept b
        WHERE
        DATE_FORMAT(a.`appointmentDate`,'%Y-%m-%d') &gt;= #{date}
        AND
